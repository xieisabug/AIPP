{
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist",
        "devUrl": "http://localhost:3000"
    },
    "bundle": {
        "active": true,
        "targets": ["msi", "deb", "rpm", "dmg", "app"],
        "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
        "resources": ["artifacts/templates/**/*"]
    },
    "productName": "Aipp",
    "mainBinaryName": "Aipp",
    "version": "0.3.0",
    "identifier": "com.xieisabug.aipp",
    "plugins": {},
    "app": {
        "trayIcon": {
            "id": "aipp",
            "iconAsTemplate": true,
            "iconPath": "icons/favicon.ico"
        },
        "security": {
            "dangerousDisableAssetCspModification": ["style-src", "img-src"],
            "csp": {
                "default-src": "'self'",
                "frame-src": "'self' http://localhost:* https://localhost:*",
                "connect-src": "'self' http://localhost:* https://localhost:* ipc: http://ipc.localhost",
                "style-src": "'self' 'unsafe-inline'",
                "font-src": "'self' data:",
                "img-src": "'self' asset: data:",
                "script-src": "'self' 'unsafe-eval'"
            },
            "assetProtocol": {
                "scope": ["**"],
                "enable": true
            }
        }
    }
}
